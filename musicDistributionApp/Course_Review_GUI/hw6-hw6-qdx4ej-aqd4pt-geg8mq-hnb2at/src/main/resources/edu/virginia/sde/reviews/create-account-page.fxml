<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.virginia.sde.reviews.CreateAccountController"
            prefHeight="720.0" prefWidth="1280.0">

    <VBox spacing="10.0" AnchorPane.topAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"
          alignment="CENTER"
    >
        <Label layoutX="289.0" layoutY="59.0" text="Create Account">
            <font>
                <Font size="24.0" />
            </font>
        </Label>
        <TextField fx:id="firstNameField"
                   maxWidth="150"
                   promptText="First Name" />
        <TextField fx:id="lastNameField"
                   maxWidth="150"
                   promptText="Last Name" />
        <TextField fx:id="userNameField"
                   maxWidth="150"
                   promptText="Username" />
        <PasswordField fx:id="passwordField"
                       maxWidth="150"
                       promptText="Password" />
        <Button mnemonicParsing="false"
                fx:id="createAccountButton"
                text="Create Account"
                onAction="#handleCreateAccountButtonAction"/>
        <Button mnemonicParsing="false"
                fx:id="backButton"
                text="Back"
                onAction="#handleBackButtonAction"/>
    </VBox>

</AnchorPane>
